                
        <div class="container">
                        <div class="col-lg-12" >
                        <div class="panel panel-primary">
                        <div class="panel-heading" align="center">
                            Formulario estudiante
                        </div>
                        <div class="panel-body">
           

               <table class="table table-striped">
      <thead>
        <tr>
          <th>Nombre</th>
          <th>Apellido</th>
          <th>Edad</th>
          <th>Promedio</th>
          <th>Opciones</th>
        </tr>
      </thead>
      <tbody>
        <% _.each(alumnos, function(alumno) { %>
      <tr>
        <td><%= alumno.Nombre %></td>
        <td><%= alumno.Apellido %></td>
        <td><%= alumno.Edad %></td>
        <td><%= alumno.Promedio%> <a class="btn btn-success " title="editar">  <p class="glyphicon glyphicon-edit"></p></a></td>
        <td>
          <a class="btn btn-danger" href="/quitar/<%=alumno.id%>">Eliminar</a>
        </td>
      </tr>
      <% }) %>  
      </tbody>
    </table>






                        </div>
                    </div>
                </div>  

        </div>